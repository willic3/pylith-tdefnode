#{boxX=1600.0*km}
#{boxY=2400.0*km}
#{boxZ=400.0*km}
#{moveX=-150.0*km}
#{moveY=0.0*km}
#{moveZ=-0.5*boxZ}
brick x {boxX} y {boxY} z {boxZ}
#{idOuter=Id("volume")}
volume {idOuter} move x {moveX} y {moveY} z {moveZ}

