# ----------------------------------------------------------------------
# Set units to SI.
# ----------------------------------------------------------------------
reset
undo off
set crash save off
#{Units('si')}

# ----------------------------------------------------------------------
# Generate geometry
# ----------------------------------------------------------------------
import Cubit 'anning_zem_geometry.cub'

# ----------------------------------------------------------------------
# Set meshing scheme.
# ----------------------------------------------------------------------
volume all scheme tetmesh
surface all scheme trimesh
#{dxInner=0.3*km}
#{dxMid=1.5*km}
#{dxOuter=40.0*km}
surface 80 79 82 81 78 size {dxOuter}
# surface 84 86 size {dxMid}
surface 94 to 163 size {dxInner}

# ----------------------------------------------------------------------
# Create volume mesh
# ----------------------------------------------------------------------
mesh surface all
# surface all smooth scheme condition number beta 2.0 cpu 5
# surface all smooth scheme centroid area pull free
# smooth surface all

mesh volume all

# ----------------------------------------------------------------------
# Do one round of smoothing.
# ----------------------------------------------------------------------
# Don't do smoothing for initial (level 1) mesh.
# remesh tet quality condition no greater than 3 inflate 2
# cleanup volume all
# volume all smooth scheme condition number beta 2.0 cpu 5
# set debug 91 on
# smooth volume all

# ----------------------------------------------------------------------
# Create nodesets and material blocks.
# ----------------------------------------------------------------------
playback 'anning_zem_bc.jou'

# ----------------------------------------------------------------------
# Export mesh.
# ----------------------------------------------------------------------
export mesh "anning_zem_var_lev1.exo" dimension 3 overwrite








